{
  "pull_request": {
    "tasks": [
      {
        "executor": "jenkins",
        "context": "functional",
        "options": {
          "jobName": "EXECUTE_AUTOMATED_TESTS",
          "parameters": {
            "tags": [
              "@smoke"
            ],
            "browsers": [
              "chrome"
            ],
            "ENVIRONMENT":"testEnv"
          }
        }
      },
      {
        "executor": "TeamCity",
        "context": "run e2e tests",
        "options": {
          "jobName": "projectName_id",
          "parameters": {
            "build_tag" : "proj_wasX",
            "ENVIRONMENT" : "env_dev",
            "cuke_env" : "env_dev|staging|prod",
            "cuke_tags" : [
              "@proj_wasX",
              "@proj_wasn"
            ]
          }
        }
      },
      {
        "executor": "travis",
        "context": "integration",
        "options": {
          "tags": [
            "@a11y"
          ]
        }
      }
    ]
  }
}
